---
import MainLayout from "@/layouts/MainLayout.astro";
import MyEvents from "@/components/vue/events/list/MyEvents.vue";

const user = Astro.locals.user;
const userId = user ? user.id : null;

if (!userId) return Astro.redirect("/signin");
---

<MainLayout title="Mis eventos">
  <section>
    <MyEvents client:load userId={userId} />
  </section>
</MainLayout>
