---
import Register from "@/components/vue/auth/Register.vue";
import MainLayout from "@/layouts/MainLayout.astro";

const user = Astro.locals.user;
const session = Astro.locals.session;

if (user && session) {
  return Astro.redirect('/events/');
}
---

<MainLayout title="Sign In">
  <div class="flex flex-col items-center justify-center p-24 mx-auto">
      <Register class="w-[350px]" client:load />
    </div>
</MainLayout>
